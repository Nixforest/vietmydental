<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="vietmydental.immortal.com.gate.g00.fragment.ConfigurationFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        >
        <ImageView
            android:layout_margin="10dp"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/infomation"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center_vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/CONTENT00072"
                android:layout_weight="1"
                android:textSize="@dimen/text_size_15"
                android:textStyle="bold"/>
            <TextView
                android:id="@+id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="VietMy Dental 1.3.9"
                android:textSize="@dimen/text_size_15"
                android:layout_marginRight="@dimen/layout_margin_small"/>


        </LinearLayout>

    </LinearLayout>

    <View
        style="@style/view_indicator_style" />

    <LinearLayout
        android:id="@+id/view_update_version"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:clickable="true"
        >
        <ImageView
            android:layout_margin="10dp"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/update_version"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/CONTENT00332"
                android:layout_weight="1"
                android:textSize="@dimen/text_size_15"
                android:textStyle="bold"/>
        </LinearLayout>

    </LinearLayout>

    <View
        style="@style/view_indicator_style" />

</LinearLayout>
